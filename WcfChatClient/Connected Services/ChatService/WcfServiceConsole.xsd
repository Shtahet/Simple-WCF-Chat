<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/WcfServiceConsole" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/WcfServiceConsole" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://schemas.datacontract.org/2004/07/System.Net" />
  <xs:complexType name="ChatUser">
    <xs:sequence>
      <xs:element xmlns:q1="http://schemas.datacontract.org/2004/07/System.Net" minOccurs="0" name="NetworkAddress" nillable="true" type="q1:IPAddress" />
      <xs:element minOccurs="0" name="NickName" nillable="true" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ChatUser" nillable="true" type="tns:ChatUser" />
  <xs:complexType name="ChatMessage">
    <xs:sequence>
      <xs:element minOccurs="0" name="Date" type="xs:dateTime" />
      <xs:element minOccurs="0" name="FromUser" nillable="true" type="tns:ChatUser" />
      <xs:element minOccurs="0" name="Message" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="ToUser" nillable="true" type="tns:ChatUser" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ChatMessage" nillable="true" type="tns:ChatMessage" />
  <xs:simpleType name="UserActions">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Join" />
      <xs:enumeration value="Left" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="UserActions" nillable="true" type="tns:UserActions" />
  <xs:complexType name="ArrayOfChatUser">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="ChatUser" nillable="true" type="tns:ChatUser" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfChatUser" nillable="true" type="tns:ArrayOfChatUser" />
</xs:schema>