<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="servicePoint" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:8686/ChatServer" binding="netTcpBinding"
                bindingConfiguration="servicePoint" contract="ChatService.IChatService"
                name="servicePoint">
                <identity>
                    <userPrincipalName value="Shtahet-PC\Derien" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>